<!DOCTYPE html>
<html>
  <head>

    <title>Anitrackgh</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'vendor.css', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'flat-admin.css', media: 'all', 'data-turbolinks-track': 'reload' %>


    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'app.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'vendor.js', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
      <%= javascript_include_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js', 'data-turbolinks-track': 'reload' %>

  </body>
</html>
