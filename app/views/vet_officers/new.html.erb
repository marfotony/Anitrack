<h2>Vet Officers</h2>
<center><%= form_for @vet_officer do |f| %>
 <% if @success %>
 	<div class="success"><%= @success %></div>

 	<% else %>

 	<div class="danger"><%= @error %></div>
 <% end %>
	<div>
		<label>Vet Officer Name</label>
		  <%= f.text_field :name %>
	</div>

	<div>
		<label>Office Location</label>
		  <%= f.text_field :location %>

	</div>
	<div>
		<label>Phone Number</label>

		  <%= f.text_field :phone_number %>

	</div>
	<div>
		
		<label>Office Address</label>
		  <%= f.text_field :address %>

	</div>
	<div>
		<label>Profile Picture</label>
		  <%= f.file_field :vprofile_pic %>

	</div>
	
	<div>
		  <%= f.submit "Add New Vet.Officer", class: "btn btn-warning" %>

	</div>
<% end %></center>